    "glob": "^10.3.10",
    "typescript": "^5.3.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.4",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.10.5",
    "tsx": "^4.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
